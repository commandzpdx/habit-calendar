{
  "env": {
    "browser": false,
    "mocha": true,
    "node": true
  },
  "extends": "airbnb-base",
  "plugins": [
    "import"
  ],
  "rules": {
    "arrow-body-style": "off",
    "arrow-parens": ["error", "always"],
    "import/no-extraneous-dependencies": "off",
    "max-len": "off",
    "no-console": "off",
    "no-underscore-dangle": "off",
    "padded-blocks": "off"
  }
}
