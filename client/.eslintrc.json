{
  "env": {
    "browser": true,
    "jest": true,
    "node": true
  },
  "extends": "airbnb",
  "plugins": [
    "import",
    "jsx-a11y",
    "react"
  ],
  "rules": {
    "arrow-body-style": "off",
    "max-len": "off",
    "no-console": "off",
    "no-underscore-dangle": "off",
    "padded-blocks": "off",
    "react/jsx-filename-extension": "off",
    "react/jsx-max-props-per-line": "off"
  }
}
